	K
	L [shape=doublecircle]
	G
	H
	I
	J
	A -> B [label=a]
	B -> C [label="ε"]
	C -> D [label=a]
	D -> E [label="ε"]
	E -> F [label=b]
	F -> K [label="ε"]
	G -> H [label=c]
	H -> L [label="ε"]
	I -> J [label=d]
	J -> L [label="ε"]
